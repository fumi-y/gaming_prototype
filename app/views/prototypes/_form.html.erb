<%= form_with model: prototype, local: true do |f|%>
  <div class="field">
    <%= f.label :name, "PrototypeName" %><br />
    <%= f.text_field :name, class: :form__text, id:"prototype_title" %>
  </div>

  <div class="field">
    <%= f.label :concept, "Concept" %><br />
    <%= f.text_field :concept, class: :form__text, id:"prototype_concept" %>
  </div>

    <div class="field">
    <%= f.label :text, "PrototypeDescription" %><br />
    <%= f.text_area :text, class: :form__text, id:"prototype_text" %>
  </div>

  <div class="field">
    <%= f.label :image, "Image" %><br />
    <%= f.file_field :image, id:"prototype_image" %>
  </div>

  <div class="field">
    <%= f.label :genre_id, "Genre" %><br />
    <%= f.collection_select(:genre_id, Genre.all, :id, :name, {}, {class:"select-box", id:"item-shipping-fee-status"}) %>
  </div>
  <div class="field">
    <%= f.label :platform_id, "Platform" %><br />
    <%= f.collection_select(:platform_id, Platform.all, :id, :name, {}, {class:"select-box", id:"item-shipping-fee-status"}) %>
  </div>
  <div class="field">
    <%= f.label :publish_id, "Publish" %><br />
      <%= f.collection_select(:publish_id, Publish.all, :id, :name, {}, {class:"select-box", id:"item-shipping-fee-status"}) %>
  </div>

  <div class="actions">
    <%= f.submit "SAVE", class: :form__btn  %>
  </div>
<% end %>